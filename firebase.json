{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "public": "public",
    "headers": [ 
      {
        "source" : "**/*.js",
        "headers" : [
          { "key" : "Cache-Control", "value" : "no-cache, must-revalidate" }
        ]
      },
      {
        "source" : "**/data/*.*",
        "headers" : [
          { "key" : "Cache-Control", "value" : "no-cache, must-revalidate" }
        ]
      }
    ]
  }
}
